{
  "name": "decorator-utils",
  "version": "1.0.1",
  "description": "Helpful utility functions to use when writing your own ES7 decorators.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukehorvat/decorator-utils.git"
  },
  "bugs": {
    "url": "https://github.com/lukehorvat/decorator-utils/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist && babel lib -d dist",
    "precommit": "npm test",
    "prepush": "npm test",
    "prepublish": "npm test && npm run build",
    "test": "mocha --compilers js:babel-register test/index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-plugin-transform-class-properties": "6.3.13",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "husky": "0.10.2",
    "mocha": "2.3.4",
    "rimraf": "2.5.0",
    "should": "8.1.1"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "keywords": [
    "decorator",
    "annotation",
    "es7",
    "es2016",
    "utilities",
    "utils",
    "helpers",
    "toolbelt",
    "functions"
  ],
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ],
    "presets": [
      "es2015"
    ]
  }
}
